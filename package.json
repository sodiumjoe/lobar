{
  "name": "lobar",
  "version": "0.8.0",
  "description": "A thin shell wrapper for lodash.chain()",
  "author": "joe moon <joe@xoxomoon.com>",
  "main": "index.js",
  "repository": "https://github.com/sodiumjoe/lobar.git",
  "scripts": {
    "clean": "/bin/rm -rf ./lib",
    "build": "npm run clean && babel src -d lib",
    "prepublish": "in-publish && npm run build || true"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "lodash",
    "shell",
    "bash",
    "jq",
    "pipe",
    "unix",
    "json"
  ],
  "bugs": "https://github.com/sodiumjoe/lobar/issues",
  "bin": {
    "lbr": "index.js"
  },
  "preferGlobal": true,
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.3",
    "copy-paste": "^1.2.0",
    "decode-keypress": "0.0.3",
    "lodash": "^4.11.1",
    "maquillage": "0.0.2",
    "rxjs": "^5.0.0-beta.7",
    "shell-quote": "^1.6.0",
    "strip-ansi": "^3.0.1",
    "yargs": "^4.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.7.0",
    "in-publish": "^2.0.0"
  }
}
